<?xml version="1.0"?>
<?xml-stylesheet href="chrome://global/skin/" type="text/css"?>

<window id="reportWindow" title="Alignment report"
	xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul"
	width="700px">

	<script type="application/x-javascript"
		src="chrome://semantic-turkey/content/alignment/validation/report.js" />
		
	<label id="reportDescription" />
	
	<listbox id="reportList" flex="1" rows="10">
		<listhead>
			<listheader label="Entity 1"/>
			<listheader label="Relation"/>
			<listheader label="Entity 2"/>
		</listhead>
		<listcols>
			<listcol flex="2"/>
			<listcol flex="1"/>
			<listcol flex="2"/>
		</listcols>
	</listbox>
	
	<hbox pack="center">
		<button id="okBtn" label="Ok" oncommand="close();" />
	</hbox>
</window>


