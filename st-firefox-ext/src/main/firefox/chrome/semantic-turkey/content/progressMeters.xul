<window title="Download in progress"
   id="progressmeters"
   xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul"
   width="180" height="2"
   maxheight="2" maxwidth="180"
   minheight="2" minwidth="180"
