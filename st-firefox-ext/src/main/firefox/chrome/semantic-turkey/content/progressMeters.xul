   	<hbox>
   		<rows>
   			<row>
   				<label value="DOWNLOAD IN PROGRESS...."/>
   			</row>
   			<row>
				<progressmeter mode="undetermined" flex="1"/>
			</row>	
		</rows>
	</hbox>
</window>